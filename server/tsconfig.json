{
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"target": "es2019",
		"lib": ["ES2019"],
		"module": "commonjs",
		"moduleResolution": "node",
		"downlevelIteration":true,
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"lib":[	
		"dom.iterable",//这里是新增项
	],
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"],
	"suppressImplicitAnyIndexErrors":true,

}